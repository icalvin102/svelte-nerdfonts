{
  "name": "svelte-nerdfonts",
  "version": "1.0.3",
  "author": "icalvin102",
  "license": "MIT",
  "description": "Use all nerdfont-icons in your Svelte project",
  "main": "components/index.js",
  "svelte": "components/index.js",
  "type": "module",
  "scripts": {
    "generate": "rm -rf icons; python nerdconvert.py -o es icons --fields iconname viewbox paths",
    "setup": "curl -O https://raw.githubusercontent.com/icalvin102/nerdconvert/master/nerdconvert.py",
    "build": "rollup -c demo/rollup.config.js"
  },
  "keywords": [
    "nerdfonts",
    "icons",
    "svelte"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icalvin102/svelte-nerdfonts.git"
  },
  "bugs": {
    "url": "https://github.com/icalvin102/svelte-nerdfonts/issues"
  },
  "homepage": "https://github.com/icalvin102/svelte-nerdfonts#readme"
}
